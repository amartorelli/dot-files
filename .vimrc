colorscheme molokai
syntax enable
"set tabstop=4
"set softtabstop=4
set number
set showcmd
set cursorline
filetype plugin indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch
set foldmethod=marker
let g:go_fmt_command = "goimports"
set laststatus=2
set foldmethod=indent
set nofoldenable
set tags=tags
